/* Program Info Dialog */
AFQProgramInfoDialog #buttonFrame {
	border : 1px solid #111;
	border-top : none;
	border-bottom : none;
}

AFQProgramInfoDialog #titleFrame {
	border-bottom : 1px solid #111;
}

AFQProgramInfoDialog #buttonBoxFrame {
	border-top : 1px solid #111;
}

AFQProgramInfoDialog #button_PrivacyPolicy,
AFQProgramInfoDialog #button_TermsConditions {
	padding-left:10px;
	text-align:left;
	font-size: 15px;
	font-style: normal;
	font-weight: 400;
	line-height: normal;
	background-color: #24272D;
}

AFQProgramInfoDialog #button_PrivacyPolicy:hover,
AFQProgramInfoDialog #button_TermsConditions:hover {
    background-color: rgba(255, 255, 255, 20%);
}

AFQProgramInfoDialog #button_PrivacyPolicy:checked,
AFQProgramInfoDialog #button_TermsConditions:checked {
    background-color: rgba(255, 255, 255, 20%);
	color:#00E0FF;
}

AFQProgramInfoDialog QTextBrowser {
	border : none;
}

QToolBarExtension {
    background: palette(button);
    min-width: 12px;
    max-width: 12px;
    padding: 4px 0px;
    margin-left: 0px;

    qproperty-icon: url(./Dark/dots-vert.svg);
}

/* Tab Widget */
QTabWidget::pane { /* The tab widget frame */
    border-top: 1px solid #111;
}

QTabWidget::tab-bar {
    alignment: left;
}

QTabBar QToolButton {
    background: rgb(44,46,53);
    border: none;
}

QTabBar::tab:top {
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
}

QTabBar::tab:bottom {
    border-bottom-left-radius: 4px;
    border-bottom-right-radius: 4px;
}

QTabBar::tab {
    background: rgba(255, 255, 255, 6%);
    color: rgba(255, 255, 255, 70%);
    border: none;
    padding: 8px 12px;
    min-width: 50px;
    margin: 1px 2px;
}

QTabBar::tab:pressed {
    background: rgba(255, 255, 255, 14%);
}

QTabBar::tab:hover {
    background: rgba(255, 255, 255, 20%);
    color: rgba(255, 255, 255, 70%);
}

QTabBar::tab:selected {
    background: rgba(255, 255, 255, 20%);
    color: #00E0FF;
}

QTabBar QToolButton {
    background: palette(button);
    min-width: 16px;
    padding: 0px;
}

/* YouTube Integration */

AFQYouTubeSettingDialog {
    qproperty-thumbPlaceholder: url(./Dark/sources/image.svg);
}

AFQYouTubeSettingDialog QScrollArea,
AFQYouTubeSettingDialog #scrollAreaWidgetContents,
AFQYouTubeSettingDialog #scrollAreaWidgetContents_3{
	background:#24272D;
}


AFQYouTubeSettingDialog QPlainTextEdit {
    border-radius: 4px;
	padding :10px;
    background-color: rgba(0, 0, 0, 0.20);
    color:rgba(255, 255, 255, 0.60);
    font-size:15px;
    font-style: normal;
    font-weight: 400;
    line-weight: normal;
    padding-left:12px;
}

AFQYouTubeSettingDialog QPlainTextEdit:focus {
    border: 2px solid #00E0FF;
    border-radius: 4px;
    background-color: rgba(0, 0, 0, 0.20);
    color:rgba(255, 255, 255, 0.60);
    font-size:15px;
    font-style: normal;
    font-weight: 400;
    line-weight: normal;
}



#ytEventList QLabel {
    color: rgba(255, 255, 255, 80%);
    background-color: rgba(255, 255, 255, 10%);
    border: none;
    border-radius: 4px;
    padding: 4px 20px;
}

#ytEventList QLabel:hover {
    background-color: rgba(255, 255, 255, 17%);
}

#ytEventList QLabel[isSelectedEvent=true] {
    background-color: rgb(40,76,184);
    border: none;
}

#ytEventList QLabel[isSelectedEvent=true]:hover {
    background-color: rgb(54,92,192);
    color: palette(text);
}

/* Calendar Widget */

/* Calendar Top Bar */
QCalendarWidget QWidget#qt_calendar_navigationbar {
    background-color: rgba(255, 255, 255, 4%);
    padding: 4px 8px;
}

/* Calendar Top Bar Buttons */
QCalendarWidget QToolButton {
    background-color: palette(button);
    padding: 2px 16px;
    border-radius: 4px;
    margin: 2px;
}

#qt_calendar_monthbutton::menu-indicator {
    image: url(./Dark/down.svg);
    subcontrol-position: right;
    padding-top: 2px;
    padding-right: 6px;
    height: 10px;
    width: 10px;
}

QCalendarWidget #qt_calendar_prevmonth {
    padding: 2px;
    qproperty-icon: url(./Dark/left.svg);
    icon-size: 16px, 16px;
}

QCalendarWidget #qt_calendar_nextmonth {
    padding: 2px;
    qproperty-icon: url(./Dark/right.svg);
    icon-size: 16px, 16px;
}

QCalendarWidget QToolButton:hover {
    background-color: rgb(79,83,94);
    border-radius: 4px;
}

QCalendarWidget QToolButton:pressed {
    background-color: rgb(25,27,38);
}

/* Month Dropdown Menu */
QCalendarWidget QMenu {

}
/* Year spinbox */
QCalendarWidget QSpinBox {
    background-color: rgb(25,27,38);
    border: none;
    border-radius: 4px;
    margin: 0px 3px 0px 0px;
    padding: 4px 16px;
}

QCalendarWidget QSpinBox::up-button { subcontrol-origin: border; subcontrol-position: top right; width: 16px; }
QCalendarWidget QSpinBox::down-button {subcontrol-origin: border; subcontrol-position: bottom right; width: 16px;}
QCalendarWidget QSpinBox::up-arrow { width: 10px; height: 10px; }
QCalendarWidget QSpinBox::down-arrow { width: 10px; height: 10px; }

/* Days of the Week Bar */
QCalendarWidget QWidget { alternate-background-color: palette(mid); }

QCalendarWidget QAbstractItemView:enabled {
    background-color: palette(base);
    color: palette(text);
}

QCalendarWidget QAbstractItemView:disabled {
    color: rgb(122,121,122);
}
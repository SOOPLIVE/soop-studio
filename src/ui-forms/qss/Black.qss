/* Common QRadioButton */
QRadioButton {
	color:rgba(255, 255, 255, 70%);
	font-size: 14px;
	font-style: normal;
	font-weight: 400;
	line-height: normal;
}

QRadioButton::indicator {
	width:18px;
	height:18px;
}
QRadioButton::indicator::checked {
	image: url("../assets/common-component/radiobutton/bt-indicator-checked.svg");
}
QRadioButton::indicator::unchecked {
	image: url("../assets/common-component/radiobutton/bt-indicator-unchecked.svg");
}

/* Common QDialogButtonBox */
QDialogButtonBox QPushButton {
	width : 128px;
	height : 40px;
}

/* Common QLabel */
QLabel {
	color:rgba(255, 255, 255, 60%);
	font-size: 15px;
	font-style: normal;
	font-weight: 400;
	line-height: normal;
}

QLabel:disabled {
	color:rgba(255, 255, 255, 18%);
}

/* Common Popup or Dock Title QLabel */
QLabel[labelType="labelTitle"] { 
	color: rgba(255, 255, 255, 90%);
	font-size: 15px;
	font-weight: 400;
}

QLabel[labelType="labelAddSourceComposit"] {
	color: rgba(255, 255, 255, 0.90);
	font-size: 14px;
	font-style: normal;
	font-weight: 700;
	line-height: normal;
}

/* Common QListWidget */
QListWidget {
    border-radius: 4px;
    padding: 3px;
    color: palette(text);
    background-color: rgb(29,31,36);
}

QListWidget::item {
    padding: 6px;	
    border-radius: 4px;
	border: 0px solid transparent;
    color: rgba(255, 255, 255, 60%);
    background-color: rgb(29,31,36);
}

QListWidget::item:selected{
    color: rgba(255, 255, 255, 90%);
    background-color: rgb(40,42,47);
}

QListWidget::item:hover,
QListWidget::item:selected:hover {
    color: rgba(255, 255, 255, 90%);
    background-color: #505257;
}

QListWidget::item:disabled,
QListWidget::item:disabled:hover {
    color: rgba(255, 0, 0, 60%);
    background-color: rgb(29,31,36);
}



/* Common RoundedDialog Style */
AFQRoundedDialogBase {
	background-color: #24272D;
	border: 1px solid #111;
    border-radius: 10px;
}

AFQPropertiesView {
	background-color: #24272D;
}

/* Common MessageBox */
QMessageBox {
	background: #24272D;
}

/* Common QToolTip Style*/
QToolTip {
	padding:2px;
    background-color: rgb(36,39,45);
    color: rgba(255, 255, 255, 90%);
	border: 1px solid #3a3d42;
}

/* Common QMenu Style */
QMenu {
    background-color: rgb(36,39,45);
	border: 1px solid #111;
    padding-left: 6px;
    padding-right: 6px;
    padding-bottom: 10px;
    padding-top: 10px;
}

QMenu::separator {
    background: #16171b;
    height: 1px;
}

QMenu::item:disabled {
    color: rgb(153,153,153);
    background: transparent;
}

QMenu::right-arrow {
    image: url(../assets/menu-spread.svg);
}

QMenu::item {
    padding-left: 10px;
    padding-right: 20px;
    padding-bottom: 6px;
    padding-top: 6px;
	color: rgba(255, 255, 255, 80%);
	font-size: 14px;
	font-style: normal;
	font-weight: 400;
	line-height: normal;
	border-radius: 4px;
}

QMenu::item:selected {
	background-color: rgba(255, 255, 255, 10%);
}

QMenu::item:hover,
QMenu::item:selected:hover {
	background-color: rgba(255, 255, 255, 10%);
}

QMenu::item:disabled{
	color: #474a4f;
}

QMenu::indicator
{
	width: 14px;
	height: 14px;
    margin : 0px 0px 0px 4px;
    padding : -4px -4px -4px -4px;
}

QMenu::indicator:non-exclusive:unchecked {
    image: url("");
}

QMenu::indicator:non-exclusive:checked {
    image: url(../assets/mainview/menu-checked.svg);
}

/* Common QLineEdit Style */
QLineEdit {
    border-radius: 4px;
	selection-background-color: #008DA3;
    background-color: rgba(255, 255, 255, 0.06);
    color:rgba(255, 255, 255, 0.60);
    font-size:15px;
    font-style: normal;
    font-weight: 400;
    padding-left:12px;
}

QLineEdit::disabled {
	background: rgba(255, 255, 255, 1.8%);
	color: rgba(255, 255, 255, 18%);
	padding-left:12px;
}

QLineEdit:hover,
QLineEdit:focus{
    border: 2px solid #00E0FF;
    border-radius: 4px;
    background-color: rgba(255, 255, 255, 0.06);
    color:rgba(255, 255, 255, 0.60);
    font-size:15px;
    font-style: normal;
    font-weight: 400;
	padding-left:10px;
}

/* Common QScrollBar Style */
QScrollBar:vertical {
    border: 2px solid grey;
    background-color:transparent;
	border:0px solid transparent;
    width: 6px;	
}

QScrollBar::handle:vertical {
    background-color: rgba(255,255,255,30%);
	border-radius:3px;
}

QScrollBar::handle:vertical:hover,
QScrollBar::handle:vertical:pressed  {
    background-color: rgba(255,255,255,50%);
	border-radius:3px;
}

QScrollBar::up-arrow:vertical, QScrollBar::down-arrow:vertical {
	background: none;
}

QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
	background: none;
}

QScrollBar::add-line:vertical {
	border: none;
	width: 0px;
	subcontrol-position: right;
	subcontrol-origin: margin;
}

QScrollBar::sub-line:vertical  {
	border: none;
	width: 0px;
	subcontrol-position: left;
	subcontrol-origin: margin;
}


QScrollBar:horizontal {
    border: 2px solid grey;
    background-color:transparent;
	border:0px solid transparent;
    height: 6px;	
}

QScrollBar::handle:horizontal {
    background-color: rgba(255,255,255,30%);
	border-radius:3px;
}

QScrollBar::handle:horizontal:hover,
QScrollBar::handle:horizontal:pressed  {
    background-color: rgba(255,255,255,50%);
	border-radius:3px;
}

QScrollBar::up-arrow:horizontal, QScrollBar::down-arrow:horizontal {
	background: none;
}

QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal {
	background: none;
}

QScrollBar::add-line:horizontal {
	border: none;
	width: 0px;
	subcontrol-position: right;
	subcontrol-origin: margin;
}

QScrollBar::sub-line:horizontal  {
	border: none;
	width: 0px;
	subcontrol-position: left;
	subcontrol-origin: margin;
}


/* Common QSlider  */
QSlider::groove:horizontal {
	background-color: #1D1F24;
	height: 4px;
	border: 1px;
	border-radius: 1px;
}

QSlider::handle:horizontal {
	background-color: #FFF;
	height: 10px;
	width: 10px;
	image: url(../assets/common-component/common-slider/slider_handle.svg);
	background: none;
	margin: -8px 0px;
}

QSlider::handle:horizontal:pressed {
	image: url(../assets/common-component/common-slider/slider_handle.svg);
}

QSlider::handle:horizontal:hover {
	image: url(../assets/common-component/common-slider/slider_handle.svg);
}

QSlider::sub-page:horizontal { 
	background-color: #00E0FF; 
	border-radius: 2px;
}


/* Common QCheckBox Style */
QCheckBox {
	color: rgba(255, 255, 255, 60%);
	font-size: 14px;
	font-style: normal;
	font-weight: 400;
	line-height: normal;
}

QCheckBox::disabled {
	color: rgba(255, 255, 255, 18%);
}

QCheckBox::indicator:unchecked:enabled {
	image: url(":/image/resource/Settings/Component/uncheckedcheckbox.svg");
}

QCheckBox::indicator:unchecked:hover {
	image: url(../assets/common-component/mousehover/uncheckedcheckbox.svg);
}

QCheckBox::indicator:unchecked:disabled {
	image: url(../assets/common-component/disabled/uncheckedcheckbox.svg);
}

QCheckBox::indicator:checked:enabled {
	image: url(":/image/resource/Settings/Component/checkedcheckbox.svg");
}

QCheckBox::indicator:checked:hover {
	image: url(../assets/common-component/mousehover/checkedcheckbox.svg);
}

QCheckBox::indicator:checked:disabled {
	image: url(../assets/common-component/disabled/checkedcheckbox.svg);
}

/* Common QDoubleSpinBox Style */
QDoubleSpinBox {
    padding-left:12px;
    padding-right:30px;
	border-radius: 4px;
	selection-background-color: #008DA3;
	background: rgba(0, 0, 0, 20%);
	color: rgba(255, 255, 255, 60%);
	font-size: 15px;
	font-weight: 400;
}

QDoubleSpinBox::disabled {
	color: rgba(255, 255, 255, 24%);
}

QDoubleSpinBox::up-button,
QDoubleSpinBox::down-button {
    width: 16px;
	height: 16px;
    border-width: 1px;
	right: 12px;
}

QDoubleSpinBox::down-button {
	bottom: 4px;
}

QDoubleSpinBox::up-button {
	top: 4px;
}

QDoubleSpinBox::up-button:hover,
QDoubleSpinBox::down-button:hover {
	background-color:rgba(255,255,255,20%);
	border-radius:4px;
}

QDoubleSpinBox::up-button:pressed,
QDoubleSpinBox::down-button:pressed {
	background: rgba(255, 255, 255, 0%);
}

QDoubleSpinBox::up-arrow,
QDoubleSpinBox::down-arrow {
	width: 12px;
}

QDoubleSpinBox::up-arrow:enabled {
    image: url(":/image/resource/Settings/Component/spinboxup.svg");
}

QDoubleSpinBox::up-arrow:disabled {
	image: url(../assets/common-component/disabled/spinboxup.svg);
}

QDoubleSpinBox::down-arrow:enabled {
    image: url(":/image/resource/Settings/Component/spinboxdown.svg");
}

QDoubleSpinBox::down-arrow:disabled {
	image: url(../assets/common-component/disabled/spinboxdown.svg);
}

/* Common QSpinBox Style */
QSpinBox {
	padding-left: 12px;
	padding-right:30px;
	border-radius: 4px;
	selection-background-color: #008DA3;
	background: rgba(0, 0, 0, 20%);
	color: rgba(255, 255, 255, 60%);
	font-size: 15px;
	font-weight: 400;
}

QSpinBox::disabled {
	color: rgba(255, 255, 255, 24%);
}

QSpinBox::up-button, 
QSpinBox::down-button {
    width: 16px;
	height: 16px;
    border-width: 1px;
	right: 12px;
}

QSpinBox::down-button {
	bottom: 4px;
}

QSpinBox::up-button {
	top: 4px;
}

QSpinBox::up-button:hover,
QSpinBox::down-button:hover {
	background-color:rgba(255,255,255,20%);
	border-radius:4px;
}

QSpinBox::down-button:pressed,
QSpinBox::up-button:pressed {
	background: rgba(255, 255, 255, 0%);
}

QSpinBox::up-arrow,
QSpinBox::down-arrow {
	width: 12px;
}

QSpinBox::up-arrow:enabled {
    image: url(":/image/resource/Settings/Component/spinboxup.svg");
}

QSpinBox::up-arrow:disabled {
	image: url(../assets/common-component/disabled/spinboxup.svg);
}

QSpinBox::down-arrow:enabled {
    image: url(":/image/resource/Settings/Component/spinboxdown.svg");
}

QSpinBox::down-arrow:disabled {
	image: url(../assets/common-component/disabled/spinboxdown.svg);
}

/* Common QComboBox Style */
QComboBox,
QDateTimeEdit {
	padding-left: 12px;
	border-radius: 4px;
	selection-background-color: #008DA3;
	background: rgba(255, 255, 255, 4%);
	color: rgba(255, 255, 255, 70%);
	font: 400 15px;
}

QComboBox::hover,
QDateTimeEdit::hover {
	background-color: rgba(255, 255, 255, 11%);
	color: rgba(255, 255, 255, 70%);
}

QComboBox::disabled,
QDateTimeEdit::disabled {
	background: rgba(255, 255, 255, 1.8%);
	color: rgba(255, 255, 255, 18%);
}

QComboBox::on,
QDateTimeEdit::on {
	border:1px solid #191919;
	background: rgba(255, 255, 255, 6%);
	border-bottom-left-radius: 0px;
	border-bottom-right-radius: 0px;
	color: rgba(255, 255, 255, 40%);
}

/* ComboBox Drop-down Arrow */
QComboBox::drop-down,
QDateTimeEdit::drop-down {
	width: 40px;
	height: 40px;
	background: transparent;
}

QComboBox::down-arrow,
QDateTimeEdit::down-arrow {
	width: 16px;
	height: 16px;
	padding: 12px;
}

QComboBox::down-arrow:enabled,
QDateTimeEdit::down-arrow:enabled {
	image: url(":/image/resource/Settings/Component/comboboxdropdown.svg");
}

QComboBox::down-arrow:disabled,
QDateTimeEdit::down-arrow:disabled {
	image: url(../assets/common-component/disabled/comboboxdropdown.svg);
}


QComboBox QAbstractItemView {
    border:none;
    border:1px solid #191919;
    outline: 0px;
    background: #24272D;
}
QComboBox QAbstractItemView::item {
	padding-left: 12px;
	height: 40px;
	background: rgba(255, 255, 255, 6%);
	color: rgba(255, 255, 255, 60%);
}
QComboBox QAbstractItemView::item:selected {
	background: rgba(255, 255, 255, 12%);
	color: #00E0FF;
}

/* Common QPushButton */
QPushButton {
    border-radius: 4px;
    background-color: rgba(255, 255, 255, 6%);
    color:rgba(255, 255, 255, 70%);
    font-size:15px;
}

QPushButton:hover {
    background-color: rgba(255, 255, 255, 20%);
}

QPushButton:pressed {
    background-color: #26292F;
	color:rgba(255, 255, 255, 14%);
}

QPushButton:disabled {
    background-color: #2B2E33;
}

QPushButton:checked {
    background-color: rgba(255, 255, 255, 20%);
}


/* Popup Title Bar Button */
QPushButton[buttonType="popupTitleBarButton"],
QPushButton[buttonType="popupTitleBarButton"]:pressed {
	background-color: rgba(255, 255, 255, 0%);
}

QPushButton[buttonType="popupTitleBarButton"]:hover {
	background-color: rgba(255, 255, 255, 10%);
}

/* Three Dots Button */
QPushButton[buttonType="threeDots"] {
	image: url("../assets/mainview/default/three-dots.svg");
	border-radius:4px;
	background-color: transparent;
}
QPushButton[buttonType="threeDots"]:hover {
	image: url("../assets/mainview/mousehover/three-dots.svg");
}

QPushButton[buttonType="hthreeDots"] {
	image: url("../assets/mainview/default/horizontalthreedots.svg");
	border-radius:4px;
	background-color: transparent;
}
QPushButton[buttonType="hthreeDots"]:hover {
	image: url("../assets/mainview/mousehover/horizontalthreedots.svg");
}

/* Close Button */
QPushButton[buttonType="closeButton"] {
	image: url("../assets/mainview/default/close.svg");
	border-radius:4px;
	background-color: transparent;
}
QPushButton[buttonType="closeButton"]:hover{
	image: url("../assets/mainview/mousehover/close.svg");
}

/* Source List Button */
QPushButton[buttonType="sourceListToolbar"],
QPushButton[buttonType="sourceListToolbar"]:pressed {
    background-color: rgba(255, 255, 255, 0%);
}

QPushButton[buttonType="sourceListToolbar"]:hover {
	background-color: rgba(255, 255, 255, 10%);
}

/* Bottom Scene 3Dot Button */
QPushButton#bottomSceneDotButton {
	background: transparent;
	border:1px solid #3a3d42;
	border-radius:5px;
}

QPushButton#bottomSceneDotButton:hover {
    background-color: rgba(255, 255, 255, 20%);
}
QPushButton#bottomSceneDotButton:pressed {
    background-color: #26292F;
}

/* Filter Popup */
QPushButton[buttonType="filterListToolbar"],
QPushButton[buttonType="filterListToolbar"]:pressed {
	background-color: #24272D;
}

QPushButton[buttonType="filterListToolbar"]:hover {
	background-color: rgba(255, 255, 255, 10%);
}

* [themeID="addFilterIcon"] {
